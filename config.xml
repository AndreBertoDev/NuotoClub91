<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nuoto.app" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Nuoto Club 91</name>
    <description>
        Nuoto club 91 app.
    </description>
    <author email="nuotoclub91pr@gmail.com" href="http://nuotoclub91parma.it">
        Andrea Bertogalli e Nicol√≤ Avanzini
    </author>
    <icon src="res/icon/icon.png" />
    <preference name="Orientation" value="portrait" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="onesignal-cordova-plugin" spec="^2.3.1" />
    <engine name="android" spec="^7.0.0" />
</widget>
